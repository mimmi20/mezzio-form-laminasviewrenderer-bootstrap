{
  "name": "mimmi20/mezzio-form-laminasviewrenderer-bootstrap",
  "type": "library",
  "description": "Provides a View Renderer for Laminas Forms to use with Bootstrap",
  "homepage": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.7 || ^8.0.0",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "laminas/laminas-escaper": "^2.7.0",
    "laminas/laminas-form": "^3.0.0",
    "laminas/laminas-json": "^3.2.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.3.0",
    "mimmi20/mezzio-laminasviewrenderer-helpers": "^1.0.5"
  },
  "require-dev": {
    "infection/infection": "^0.23.0",
    "laminas/laminas-i18n": "^2.11.1",
    "laminas/laminas-servicemanager": "^3.6.4",
    "mimmi20/coding-standard": "^2.2.2",
    "mimmi20/laminas-form-element-group": "^1.0.2",
    "mimmi20/laminas-form-element-links": "^1.0.0",
    "mimmi20/laminas-form-element-paragraph": "dev-master",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.89",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.20",
    "phpunit/phpunit": "^9.5.6"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
    "laminas/laminas-servicemanager": "^3.6.4, to use the navigation factories",
    "mimmi20/laminas-form-element-group": "^1.0.0, to render fieldsets just for styling",
    "mimmi20/laminas-form-element-links": "to render links inside a form",
    "mimmi20/laminas-form-element-paragraph": "to render paragraphs inside a form"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\BootstrapForm\\LaminasView\\View\\Helper\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\BootstrapForm\\LaminasView\\View\\Helper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\BootstrapForm\\LaminasView\\View\\Helper\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap/issues",
    "source": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap"
  }
}
