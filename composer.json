{
    "name": "mimmi20/mezzio-form-laminasviewrenderer-bootstrap",
    "description": "Provides a View Renderer for Laminas Forms to use with Bootstrap",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap",
    "support": {
        "issues": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap/issues",
        "source": "https://github.com/mimmi20/mezzio-form-laminasviewrenderer-bootstrap"
    },
    "require": {
        "php": "^8.1.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laminas/laminas-form": "^3.17.0",
        "laminas/laminas-json": "^3.6.0",
        "mezzio/mezzio-laminasviewrenderer": "^2.15.0",
        "mimmi20/laminasviewrenderer-helper-htmlelement": "^2.0.5",
        "mimmi20/laminasviewrenderer-helper-partialrenderer": "^2.0.0",
        "psr/container": "^1.1.2 || ^2.0.2"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.9",
        "laminas/laminas-i18n": "^2.24.1",
        "laminas/laminas-servicemanager": "^3.22.1",
        "mimmi20/coding-standard": "^5.2.3",
        "mimmi20/laminas-form-element-group": "^3.0.6",
        "mimmi20/laminas-form-element-links": "^2.0.5",
        "mimmi20/laminas-form-element-paragraph": "^2.0.5",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.50",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.5.3",
        "rector/rector": "^0.18.13"
    },
    "suggest": {
        "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
        "laminas/laminas-servicemanager": "^3.6.4, to use the form factories",
        "mimmi20/laminas-form-element-group": "^1.0.4, to render fieldsets just for styling",
        "mimmi20/laminas-form-element-links": "^1.0.2, to render links inside a form",
        "mimmi20/laminas-form-element-paragraph": "^1.0.1, to render paragraphs inside a form"
    },
    "autoload": {
        "psr-4": {
            "Mimmi20\\Mezzio\\BootstrapForm\\LaminasView\\View\\Helper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mimmi20Test\\Mezzio\\BootstrapForm\\LaminasView\\View\\Helper\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "laminas": {
            "config-provider": "Mezzio\\BootstrapForm\\LaminasView\\View\\Helper\\ConfigProvider"
        }
    }
}
